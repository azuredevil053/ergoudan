<view class="index-container {{ indoor ? 'indoor' : '' }}">
    <view class="toolbar">
        <view class="search">
            <view class='iconfont icon-search'></view>
            <view class='text'>输入设施编号或者负责人</view>
            <view class='iconfont icon-scan' bindtap="onScanCode"></view>
        </view>
        <view class="add">
            <view class="iconfont {{ indoor ? 'icon-coordinates_fill' : 'icon-jianzhu'}}" bindtap="onActionSheet"></view>
        </view>
        <view class="add" bindtap="enterClick">
            <view class='iconfont icon-addition_fill'></view>
        </view>
    </view>
