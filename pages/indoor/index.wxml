        <map class="map"
             id="map"
             show-compass="true"
             enable-3D="true"
             show-compass="true"
             enable-rotate="true"
             longitude="{{longitude}}"
             latitude="{{latitude}}"
             scale="{{scale}}"
             show-location="true"
             markers="{{markers}}"
             bindmarkertap="onMakerTap"
             bindtap="bindTap"
        >
