.index-container{
    font-size: 28rpx;
}

.map_container{
    position: relative;
    width: 100%;
    height: calc(100vh - 80rpx);
    left: 0;
    z-index: 2;
}
.index-container.indoor .map_container{
    position: relative;
    width: 100%;
    height: 30vh;
    left: 0;
    z-index: 2;
}
.map_container .map{
    height: 100%;
    width: 100%;
    z-index: 2;
}
.index-container .toolbar{
    background-color: #409EFF;
    display: flex;
    align-items: center;
    padding: 0 20rpx 20rpx 20rpx;
}
.index-container .toolbar >view{
    margin-right: 20rpx;
}
.index-container .toolbar .add{
    height: 60rpx;
    /*background-color: rgba(255,255,255,0.2);*/
    border-radius: 30rpx;
    line-height: 60rpx;
    padding: 0 6rpx;
}
.index-container .toolbar .add .iconfont{
    /*color: #ffffff;*/
    /*height: 60rpx;*/
    /*border: 1px solid #ffffff;*/
    /*border-radius: 8px;*/
    /*line-height: 60rpx;*/
    /*padding: 0 10px;*/
    font-size: 54rpx;
    color: #ffffff;
    position: relative;
    top: 1rpx;

}
.index-container .toolbar >view:last-child{
    margin-right: 0;
}
.index-container .toolbar .search{
    display: flex;
    align-items: center;
    height: 60rpx;
    width: 100%;
    background-color: rgba(255,255,255,0.2);
    border-radius: 30rpx;
}
.index-container .toolbar .search .iconfont{
    line-height: 60rpx;
    margin-left: 20rpx;
    color: #ffffff;
    margin-right: 20rpx;
    font-size: 46rpx;
}
.index-container .toolbar .search .text{
    width: 100%;
    color: white;
    margin-right: 20rpx;
}
.index-container .list-title{
    line-height: 70rpx;
    padding-left: 30rpx;
    color: #666666;
    background: #f5f5f5;
}
.index-container .list-title text{
    color: #409EFF;
    margin: 0 5rpx;
}
.index-container .build-list{
    padding: 10rpx 0;
    background: #ffffff;
}
.index-container .build-list .item{
    border-bottom: 1px #efefef solid;
}
.index-container .build-list .item:last-child{
    border-bottom: 0;
}
.index-container .build-list .item .item-bd{
    display: flex;
    align-items: center;
    padding: 10rpx 30rpx;
}
.index-container .build-list .item .item-bd .iconfont{
    font-size: 50rpx;
}
.index-container .build-list .item .item-bd .icon-jianzhu{
    line-height: 70rpx;
    width: 70rpx;
    background: #efefef;
    border-radius: 50%;
    text-align: center;
    color: rgba(153, 166, 0, 0.78);
}
.index-container .build-list .item .item-bd .text{
    margin-right: auto;
    margin-left: 20rpx;
    color: #666666;
}
.index-container .build-list .item .item-bd .text .adds{
    font-size: 24rpx;
    color: #999999;
    margin-top: 8rpx;
}
.index-container .build-list .item .item-bd .num{
    color: #409EFF;
    margin-right: 20rpx;
}

.index-container .build-list .err .item-bd .icon-jianzhu, .index-container .build-list .err .item-bd .num{
    color: #ff2327;
}
.index-container .build-list .item .item-bd .icon-addition{
    color: #999999;
}
.index-container .build-list .item .item-list{
    height: 0;
    overflow: hidden;
}
.index-container .build-list .item .item-list .item-m{

    display: none;
    color: #666666;
    line-height: 60rpx;
    border-bottom: 1px #efefef solid;
    margin: 0 30rpx 0 120rpx;
}
.index-container .build-list .item .item-list .item-m:last-child{
    border-bottom: 0;
}
.index-container .build-list .item .item-list .item-m .name{
    margin-right: auto;
}
.index-container .build-list .item-show .item-list .item-m{
    display: flex;
}
.index-container .build-list .item-show .item-list{
    height: auto;
}
.index-container .list-title, .index-container .build-list{
    display: none;
}
.index-container.indoor .list-title, .index-container.indoor .build-list{
    display: block;
}
